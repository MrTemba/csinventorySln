@model IEnumerable<bool>
<a class="btn btn-block btn-outline-secondary" asp-action="Index" 
        asp-controller="Home" asp-route-category="">
    Home
</a>
@foreach (bool category in Model)
{


<a class="btn btn-block @(category.ToString() == ViewBag.SelectedCategory? "btn-primary": "btn-outline-secondary")"
   asp-action="Index" asp-controller="Home"
   asp-route-category="@category"
   asp-route-partPage="1">
    @(category?"Reworkables" : "Non-Reworkables")
</a>
}